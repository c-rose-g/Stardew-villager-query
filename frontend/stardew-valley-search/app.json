{
  "expo": {
    "name": "stardew-valley-search",
    "slug": "stardew-valley-search",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "foregroundImage": "./assets/images/index-bg.png",
      "resizeMode": "cover"
    },
    "ios": {
      "buildNumber": "3",
      "supportsTablet": false,
      "userInterfaceStyle": "automatic",
      "splash": {
        "image": "./assets/images/index-bg.png",
        "resizeMode": "cover"
      },
      "bundleIdentifier": "com.stardewvalley.search"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/index-bg.png",
          "resizeMode": "cover"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "b9f02597-9ad6-4da3-bdc4-e7bf7e0dfe01"
      }
    }
  }
}
