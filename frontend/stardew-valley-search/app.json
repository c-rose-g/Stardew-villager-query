{
  "expo": {
    "name": "stardew-valley-search",
    "slug": "stardew-valley-search",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "foregroundImage": "./assets/images/index-bg.png"
    },
    "ios": {
      "supportsTablet": false,
      "userInterfaceStyle":"automatic",
      "splash":{
        "image":"./assets/images/index-bg.png"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      ["expo-splash-screen",
      {
        "image":"./assets/images/index-bg.png",
        "resizeMode": "cover"
      }]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
